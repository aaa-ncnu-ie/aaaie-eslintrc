{
  "strict": [
    2,
    "function"
  ],
  "eqeqeq": [
    2,
    "smart"
  ],
  "no-redeclare": [
    2,
    {
      "builtinGlobals": true
    }
  ],
  "no-use-before-define": [
    2,
    "nofunc"
  ],
  "comma-style": [
    2,
    "last"
  ],
  "no-multiple-empty-lines": [
    2,
    {
      "max": 3,
      "maxEOF": 5
    }
  ],
  "indent": [
    "error",
    2,
    {
      "SwitchCase": 1
    }
  ],
  "quotes": [
    "error",
    "single"
  ],
  "semi": [
    "error",
    "always"
  ],
  "arrow-body-style": [
    "error",
    "always"
  ],
  "space-before-function-paren": [
    "error",
    {
      "anonymous": "always",
      "named": "never"
    }
  ],
  "no-warning-comments": [
    1,
    {
      "terms": [
        "todo",
        "fixme",
        "fix"
      ],
      "location": "start"
    }
  ],
  "no-shadow": [
    1,
    {
      "builtinGlobals": false,
      "hoist": "functions",
      "allow": []
    }
  ],
  "no-mixed-spaces-and-tabs": [
    1,
    "smart-tabs"
  ],
  "quote-props": [
    1,
    "consistent-as-needed",
    {
      "keywords": true
    }
  ],
  "jsx-quotes": [
    2,
    "prefer-double"
  ],
  "keyword-spacing": [
    2,
    {
      "before": true,
      "after": true
    }
  ],
  "space-before-blocks": 2,
  "object-curly-spacing": ["error", "always"],
  "no-useless-constructor": 2,
  "comma-dangle": 2,
  "no-extra-boolean-cast": 2,
  "no-extra-semi": 2,
  "no-sparse-arrays": 2,
  "no-unreachable": 2,
  "valid-typeof": 2,
  "curly": 2,
  "no-eval": 2,
  "no-implied-eval": 2,
  "no-loop-func": 2,
  "no-delete-var": 2,
  "no-undefined": 2,
  "no-trailing-spaces": 2,
  "no-const-assign": 2,
  "no-var": 1,
  "no-extra-parens": 1,
  "prefer-const": 1,
  "no-unused-vars": 1,
  "no-undef": 1,
  "no-unexpected-multiline": 1,
  "no-spaced-func": 1,
  "block-scoped-var": 1,
  "default-case": 1,
  "no-alert": 1,
  "no-fallthrough": 1,
  "no-floating-decimal": 1,
  "no-multi-spaces": 1,
  "vars-on-top": 1,
  "camelcase": 1,
  "no-nested-ternary": 1,
  "init-declarations": 0,
  "no-console": 0,
  "no-inline-comments": 0,
  "no-underscore-dangle": 0,
  "linebreak-style": 0
}
